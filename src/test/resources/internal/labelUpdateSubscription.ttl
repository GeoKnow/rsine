@prefix spin: <http://spinrdf.org/sp/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rsine: <http://lod2.eu/rsine/> .

[] a rsine:Subscription;
    rsine:query [
        spin:text "PREFIX cs:<http://purl.org/vocab/changeset/schema#>
            PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
            PREFIX skos:<http://www.w3.org/2004/02/skos/core#>

            SELECT ?oldLabel ?newLabel WHERE {
                ?addCs cs:createdDate ?addCsDate .
                ?remCs cs:createdDate ?remCsDate .

                ?addCs cs:addition ?addition .
                ?remCs cs:addition ?removal .

                ?removal rdf:subject ?concept .
                ?removal rdf:predicate skos:prefLabel .
                ?removal rdf:object ?oldLabel .

                ?addition rdf:subject ?concept .
                ?addition rdf:predicate skos:prefLabel .
                ?addition rdf:object ?newLabel .

                FILTER (?remCsDate > 'QUERY_LAST_ISSUED'^^<http://www.w3.org/2001/XMLSchema#dateTime> &&
                        ?oldLabel != ?newLabel &&
                        seconds(?remCsDate) - seconds(?addCsDate) < 2 &&
                        minutes(?remCsDate) = minutes(?addCsDate) &&
                        hours(?remCsDate) = hours(?addCsDate) &&
                        day(?remCsDate) = day(?addCsDate) &&
                        month(?remCsDate) = month(?addCsDate) &&
                        year(?remCsDate) = year(?addCsDate))
            }";

        rsine:formatter [
            a rsine:vtlFormatter;
            rsine:message "'$bindingSet.getValue('oldLabel')' -> '$bindingSet.getValue('newLabel')', diff: '$bindingSet.getValue('dateDiff')'";
        ]

    ].
